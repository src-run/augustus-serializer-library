{
  "name": "src-run/augustus-serializer-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/augustus-serializer-library",
  "description": "A simple serialization abstraction with support for php- json- and igbinary-based serialization.",
  "support": {
    "email": "rmf@src.run",
    "issues": "https://src.run/augustus-serializer-library/issues",
    "source": "https://src.run/augustus-serializer-library/git"
  },
  "keywords": [ "augustus-serializer-library", "augustus", "serializer", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "~8.1",
    "src-run/augustus-utility-library": "~0.20.0"
  },
  "require-dev": {
    "ext-igbinary": "*",
    "codacy/coverage": "~1.4",
    "friendsofphp/php-cs-fixer": "~3.2",
    "php-coveralls/php-coveralls": "~2.4",
    "symfony/phpunit-bridge": "~4.1 | ~5.0 | ~6.0",
    "symfony/var-dumper": "~4.1 | ~5.0 | ~6.0"
  },
  "suggest": {
    "ext-igbinary": "Required to use igbinary serializer implementation."
  },
  "autoload": {
    "psr-4": {
      "SR\\Serializer\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Serializer\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.20-dev"
    }
  }
}
